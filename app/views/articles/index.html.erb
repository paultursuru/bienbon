<% @articles.each do |article|%>
  <% if article.visible %>
    <div class="article-card">
      <%= link_to "#{article.title}", article_path(article), class: "article-title" %>
      <%= link_to "télécharger", article.url, class: "article-url" %>
      <% if user_signed_in? %>
        <%= link_to "modifier", edit_article_path(article), class: "article-edit"%>
      <% end %>
    </div>
  <% end %>
<% end %>
